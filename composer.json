{
    "name": "marcel-mathias-nolte/ken-burns-slideshow",
    "type": "contao-bundle",
    "description": "Provides a slideshow with Ken-Burns-effect which is shown in fullscreen mode",
    "homepage": "https://github.com/marcel-mathias-nolte",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Leo Feyer",
            "homepage": "https://github.com/leofeyer"
        },
        {
            "name": "Contao Community",
            "homepage": "https://contao.org/contributors"
        },
        {
            "name": "Marcel Mathias Nolte",
            "homepage": "https://github.com/marcel-mathias-nolte"
        }
    ],
    "require": {
        "php": "^7.1",
        "contao/core-bundle": "^4.4",
        "symfony/framework-bundle": "^3.3"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.12",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message-factory": "^1.0.2",
        "phpunit/phpunit": "^5.7.26",
        "symfony/phpunit-bridge": "^3.2"
    },
    "extra": {
        "contao-manager-plugin": "MarcelMathiasNolte\\KenBurnsSlideshowBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "MarcelMathiasNolte\\KenBurnsSlideshowBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MarcelMathiasNolte\\KenBurnsSlideshowBundle\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/marcel-mathias-nolte/ken-burns-slideshow/issues",
        "source": "https://github.com/marcel-mathias-nolte/ken-burns-slideshow"
    }
}
